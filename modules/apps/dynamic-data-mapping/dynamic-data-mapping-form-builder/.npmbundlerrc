		"metal-dom": true,
		"metal-events": true,
		"metal-jsx": true,
		"metal-soy": true,
		"metal-state": true,
		"metal": true
	},
	"ignore": [
		"**/*.js",
		"**/__tests__/*.js",
		"!**/*.es.js",
		"!**/*.soy.js"
	],
	"preset": "liferay-npm-bundler-preset-liferay-dev"
